> t(B)
> tB <- t(B)

#Input vectors.
> a <- replicate(10,as.integer(abs(rnorm(10)*10)))
> b <- replicate(10,as.integer(abs(rnorm(100)*100)))

#Multiply inverse with vector.
> A2 <- tA %*% a
> B2 <- tB %*% b

#Create new matrices with vectors.
> a2 <- matrix(a, nrow= 10)
> b2 <- matrix(b, nrow= 10)
